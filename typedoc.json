{
  "entryPoints": ["src"],

  "excludeInternal": true,
  "externalSymbolLinkMappings": {
    "@types/web": {
      "PermissionDescriptor": "https://developer.mozilla.org/docs/Web/API/Permissions/query#permissiondescriptor",
      "PermissionState": "https://developer.mozilla.org/docs/Web/API/PermissionStatus/state"
    }
  },
  "navigationLinks": {
    "GitHub": "https://github.com/ezzatron/fake-permissions"
  },
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "rewrittenLink": true,
    "notDocumented": true,
    "unusedMergeModuleWith": true
  },

  "plugin": ["typedoc-plugin-mdn-links"]
}
